{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc" : 0,
        "linebreak-style": 0,
        "max-len": 0,
        "quotes": 0,
        "spaced-comment": 0,
        "object-curly-spacing": 0,
        "no-unused-vars": 0,
        "comma-dangle": 0,
        "valid-jsdoc": 0
    }
}
